<?xml version="1.0" encoding="UTF-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee                http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <!-- Servlets -->
 
  <servlet>
    <servlet-name>uploadImage</servlet-name>
    <servlet-class>ch.unibe.mcs.celebfinder.server.UploadImageServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>uploadImage</servlet-name>
    <url-pattern>/uploadImage</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>suggestName</servlet-name>
    <servlet-class>ch.unibe.mcs.celebfinder.server.SuggestNameServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>suggestName</servlet-name>
    <url-pattern>/suggestName</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>getImage</servlet-name>
    <servlet-class>ch.unibe.mcs.celebfinder.server.GetImageServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>getImage</servlet-name>
    <url-pattern>/getImage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>imageSelected</servlet-name>
    <servlet-class>ch.unibe.mcs.celebfinder.server.ImageSelectedServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>imageSelected</servlet-name>
    <url-pattern>/imageSelected</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>ch.unibe.mcs.celebfinder.server.UserLoginServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value/>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>

</web-app>