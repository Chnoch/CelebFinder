  <servlet>
    <servlet-name>uploadImage</servlet-name>
    <servlet-class>ch.unibe.mcs.celebfinder.server.UploadImageServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>uploadImage</servlet-name>
    <url-pattern>/uploadImage</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>randomImage</servlet-name>
    <servlet-class>ch.unibe.mcs.celebfinder.server.RandomImageServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>randomImage</servlet-name>
    <url-pattern>/randomImage</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>suggestName</servlet-name>
