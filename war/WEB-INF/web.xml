  <servlet-mapping>
    <servlet-name>suggestName</servlet-name>
    <url-pattern>/suggestName</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>getImage</servlet-name>
    <servlet-class>ch.unibe.mcs.celebfinder.server.GetImageServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>getImage</servlet-name>
    <url-pattern>/getImage</url-pattern>
